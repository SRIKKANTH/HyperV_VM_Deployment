<Config>
	<global>
		<imageStoreDir></imageStoreDir>
		<isCluster>false</isCluster>
	</global>
	<VMs>
		<VM>
			<hvServer>localhost</hvServer>
			<vmName>airflow-backend</vmName>
			<Count>1</Count>
			<ipv4></ipv4>
			<userName>root</userName>
			<passWord>SamplePassword</passWord>
			<StartupScript>SampleScript.sh</StartupScript>
			<hardware>
				<create>true</create>
				<numCPUs>2</numCPUs>
				<memSize>2048</memSize>
				<disableDiff>false</disableDiff>
				<parentVhd>UbuntuBase.vhd</parentVhd>
				<network>
					<nic>VMBUS,PrivateSwitch</nic>
					<nic>VMBUS,InternalSwitch</nic>
					<nic>VMBUS,USB</nic>
				</network>
				<generation>1</generation>
				<secureBoot>false</secureBoot>
				<DataVhd></DataVhd>
			</hardware>
		</VM>
		<VM>
			<hvServer>localhost</hvServer>
			<vmName>airflow-master</vmName>
			<Count>1</Count>
			<ipv4></ipv4>
			<userName>root</userName>
			<passWord>SamplePassword</passWord>
			<StartupScript>SampleScript.sh</StartupScript>
			<hardware>
				<create>true</create>
				<numCPUs>2</numCPUs>
				<memSize>2048</memSize>
				<disableDiff>false</disableDiff>
				<parentVhd>UbuntuBase.vhd</parentVhd>
				<network>
					<nic>VMBUS,InternalSwitch</nic>
					<nic>VMBUS,Ethernet</nic>
					<nic>VMBUS,USB</nic>
				</network>
				<generation>1</generation>
				<secureBoot>false</secureBoot>
				<DataVhd></DataVhd>
			</hardware>
		</VM>
		<VM>
			<hvServer>localhost</hvServer>
			<vmName>airflow-worker</vmName>
			<Count>1</Count>
			<ipv4></ipv4>
			<userName>root</userName>
			<passWord>SamplePassword</passWord>
			<StartupScript>SampleScript.sh</StartupScript>
			<hardware>
				<create>true</create>
				<numCPUs>2</numCPUs>
				<memSize>2048</memSize>
				<disableDiff>false</disableDiff>
				<parentVhd>UbuntuBase.vhd</parentVhd>
				<network>
					<nic>VMBUS,InternalSwitch</nic>
					<nic>VMBUS,Ethernet</nic>
					<nic>VMBUS,USB</nic>
				</network>
				<generation>1</generation>
				<secureBoot>false</secureBoot>
				<DataVhd></DataVhd>
			</hardware>
		</VM>
	</VMs>
	<TEST>
		<TEST_APP_ZIP></TEST_APP_ZIP>
	</TEST>
</Config>
